{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/wordpress-block-carousel",
	"version": "0.1.0",
	"title": "Wordpress Block Carousel",
	"category": "widgets",
	"icon": "cover-image",
	"description": "Example block scaffolded with Create Block tool.",
	"attributes": {
		"slideData" : {
			"type": "array",
			"default": [
				{
					"backgroundImageUrl": "",
					"backgroundImageAltText": "",
					"showFocalPointPicker": "",
					"imperativeFocalPointPreview": "",
					"focalPoint": {"x": 0.5, "y": 0.5},
					"hasParallax": false
				}
			]
		},
		"slideAmount": {
			"type": "number",
			"default": 1
		},
		"currentSlide": {
			"type": "number",
			"default": 0
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "wordpress-block-carousel",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
